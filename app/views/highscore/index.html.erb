<div class="jumbotron jumbotron-fluid">
    <h1>Your Profile</h1>
    <%= image_tag(current_user.image, :alt => "rss feed") %><br>
    <strong>Score:<%=current_user.score%><br>
</div>
<ul class="list-group">
<br>
<div class="col-md-offset-5 col-md-2">
    <h1> Max Score </h1>
    <% @users.each do |user|%>
        <li class="list-group-item">
            <ul class="list-inline">
                <li class="list-inline-item"><%= image_tag(user.image, :alt => "rss feed") %></li>
                <li class="list-inline-item"><strong></strong><%=user.name%></li>
                <li class="list-inline-item"><%=user.score%></li>
            </ul>
        </li>
    <%end%>
    <h1> Min Score</h1>
    <% @max_users.each do |user|%>
        <li class="list-group-item">
            <ul class="list-inline">
                <li class="list-inline-item"><%= image_tag(user.image, :alt => "rss feed") %></li>
                <li class="list-inline-item"><strong></strong><%=user.name%></li>
                <li class="list-inline-item"><%=user.score%></li>
            </ul>
        </li>
    <%end%>
</div>